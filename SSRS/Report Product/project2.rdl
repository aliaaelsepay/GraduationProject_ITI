  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource3</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SelectedCategoryName">
            <Value>=Parameters!SelectedCategoryName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SelectedSubCategoryName">
            <Value>=Parameters!SelectedSubCategoryName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
  c.categoryname , p.Brand,
    p.ProductName,
    p.Rating
 , p.productID, p.mainingredient FROM Product p
JOIN SubCategory sc ON p.SubCategoryID = sc.SubCategoryID
JOIN Category c ON sc.CategoryID = c.CategoryID
WHERE c.CategoryName = @SelectedCategoryName
  AND sc.SubCategoryName = @SelectedSubCategoryName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
