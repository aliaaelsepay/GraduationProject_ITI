  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SelectedCategoryName">
            <Value>=Parameters!SelectedCategoryName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
    p.Brand,
    p.ProductName,
    p.Rating
FROM Product p
JOIN SubCategory sc ON p.SubCategoryID = sc.SubCategoryID
JOIN Category c ON sc.CategoryID = c.CategoryID
WHERE c.CategoryName = @SelectedCategoryName
</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Brand">
          <DataField>Brand</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ProductName">
          <DataField>ProductName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Rating">
          <DataField>Rating</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
